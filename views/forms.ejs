<%- include('partials/header') %>

    <body>
        <div class="quest-container">
            <table>
                <tr>
                    <th><h3>TÃ­tulo:</h3></th>
                    <th><h3>Respondido por:</h3></th>
                    <th><h3>Data de cadastro:</h3></th>
                    <th><h3>Latitude / Longitude:</h3></th>
                </tr>
                <% formData.forEach(function(form){ %>
                    <tr>
                        <th><a href="/answer/<%= form.answerIndex %>"><%= form.formTitle %> </a></th>
                        <th><%= form.userName %></th>
                        <th><%= form.date %></th>
                        <th><%= parseFloat(form.latitude).toFixed(4) %> / <%= parseFloat(form.longitude).toFixed(4) %></th>    
                    </tr>
                <% }); %>
            </table>
        </div>
    </body>
</html>